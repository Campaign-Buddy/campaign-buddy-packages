    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-webpack5-compiler-swc": "3.0.0",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/react-webpack5": "8.6.14",
    "@types/babel__core": "^7.1.14",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^2.0.7",
    "commander": "^8.3.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^11.4.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^16.2.0",
    "googleapis": "^73.0.0",
    "husky": "^8.0.0",
    "immutability-helper": "^3.1.1",
    "json5": "^2.2.1",
    "lerna": "^6.6.1",
    "prettier": "^2.6.2",
    "prettier-eslint": "^14.0.1",
    "react": "^18.0.9",
    "react-dom": "^18.0.9",
    "storybook": "8.6.14",
    "styled-components": "^5.3.3",
    "tsx": "^4.19.4",
    "typescript": "^4.2.4",
    "typescript-eslint": "^8.33.1",
    "vitest": "^3.1.3"
  },
  "scripts": {
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider storybook dev -p 6006",
    "build-storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider storybook build -o docs-build",
    "google": "ts-node --transpile-only tools/getGoogleAccessToken.ts",
    "test": "lerna run test",
    "build": "lerna run build",
    "lint": "eslint . --quiet",
    "new-package": "node ./tools/newPackage/newPackage.js -p",
    "new-react-package": "node ./tools/newPackage/newPackage.js -r -p",
    "audit-deps": "tsx tools/auditCrossDependencies.ts",
    "modify-tsconfigs": "tsx tools/modifyTsConfigs.ts",
    "prettier": "prettier --write ./packages",
    "prepare": "husky install"
  },
  "packageManager": "yarn@4.9.1"
}
