{
  "name": "campaign-buddy-frontend",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "apps/*"
    ]
  },
  "type": "module",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "repository": "https://github.com/Campaign-Buddy/campaign-buddy-frontend",
  "devDependencies": {
    "@babel/core": "^7.13.16",
